{
  "name": "@nsa/aws",
  "private": true,
  "version": "0.0.0",
  "description": "Nsl SAAS Accelerator on AWS",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "workspaces": {
    "packages": [
      "packages/constructs/lib/aws-codecommit/codecommit-secret",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build --stream",
    "cleanup": "lerna run cleanup --stream && rm -rf node_modules",
    "cleanup:tsc": "lerna run cleanup:tsc --stream",
    "docs": "yarn run typedoc --entryPointStrategy packages './packages/*' --name 'NSL SAAS Accelerator on AWS' --includeVersion --disableSources --logLevel Verbose",
    "postinstall": "npx husky install .husky",
    "lint": "lerna run lint --stream",
    "prepare": "npx husky install .husky",
    "prettier": "lerna run prettier --stream",
    "test": "lerna run test --stream",
    "test:clean": "rm -f ./test-reports/*.xml",
    "viper-scan": "aws s3 cp s3://viperlight-scanner/latest/viperlight.zip . && unzip -qo viperlight.zip -d ../viperlight && rm -r ./viperlight.zip && ../viperlight/bin/viperlight scan"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "^15.3.5",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.15.11",
    "@types/prettier": "2.7.2",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "graceful-fs": "^4.2.11",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "2.0.0",
    "json-schema-to-typescript": "^12.0.0",
    "lerna": "^6.6.1",
    "nx": "15.9.2",
    "patch-package": "^6.5.1",
    "prettier": "2.8.7",
    "semver": "^7.5.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typescript": "~5.0.4"
  },
  "resolutions": {
    "aws-cdk": "2.72.0",
    "aws-cdk-lib": "2.72.0"
  },
  "packageManager": "yarn@3.5.0"
}
