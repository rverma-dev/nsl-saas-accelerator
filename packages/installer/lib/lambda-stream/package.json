  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "18.15.11",
    "aws-lambda": "^1.0.7",
    "esbuild": "0.17.17",
    "eslint": "^8.38.0",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1360.0"
  }
}
