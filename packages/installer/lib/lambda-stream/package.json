{
  "name": "@nsa/dynamo-stream",
  "version": "0.0.0",
  "description": "Custom resource Lambda",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "cleanup": "rm -rf node_modules && rm -rf yarn.lock && rm -rf dist && rm -rf cdk.out",
    "prebuild": "rm -rf dist",
    "build": "esbuild index.ts --minify --bundle --sourcemap --platform=node --target=es2020 --outfile=./dist/index.js",
    "postbuild": "cd dist && zip -r index.zip index.js*",
    "test": "",
    "lint": "eslint --fix  --max-warnings 0 -c ../../../../.eslintrc.json '**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../../../.eslintrc.json '**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" "
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "18.15.11",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0"
  },
  "dependencies": {
    "@aws-sdk/client-codebuild": "^3.315.0",
    "@aws-sdk/client-dynamodb": "^3.315.0",
    "@aws-sdk/config-resolver": "^3.310.0"
  }
}
