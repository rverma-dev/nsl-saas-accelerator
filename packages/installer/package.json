{
  "name": "@nsa/installer",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -rf yarn.lock && rm -rf dist && rm -rf cdk.out",
    "cleanup:tsc": "tsc --build ./ --clean",
    "lint": "eslint --fix  --max-warnings 0 -c ../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "test": "jest --coverage --ci",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.316.0",
    "@aws-sdk/client-codepipeline": "3.316.0",
    "@aws-sdk/client-dynamodb": "3.316.0",
    "@aws-sdk/client-ec2": "3.316.0",
    "@nsa/demo": "workspace:^",
    "aws-cdk-lib": "2.76.0",
    "cdk-nag": "2.25.16",
    "constructs": "10.2.1",
    "source-map-support": "^0.5.21",
    "vm2": "^3.9.17"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "aws-cdk": "2.76.0",
    "jest": "29.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4"
  }
}
