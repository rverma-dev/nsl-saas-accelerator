{
  "name": "@nsa/installer",
  "version": "0.1.0",
  "private": true,
  "bin": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -rf yarn.lock && rm -rf dist && rm -rf cdk.out",
    "cleanup:tsc": "tsc --build ./ --clean",
    "lint": "eslint --fix  --max-warnings 0 -c ../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "test": "jest --coverage --ci",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.315.0",
    "@aws-sdk/client-codepipeline": "3.315.0",
    "@aws-sdk/client-dynamodb": "3.315.0",
    "@aws-sdk/client-ec2": "3.315.0",
    "@nsa/demo": "workspace:^",
    "aws-cdk-lib": "2.72.0",
    "cdk-nag": "2.25.11",
    "constructs": "10.0.12",
    "source-map-support": "^0.5.21",
    "vm2": "^3.9.17"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@types/node": "18.15.11",
    "aws-cdk": "2.72.0",
    "jest": "29.5.0"
  }
}
