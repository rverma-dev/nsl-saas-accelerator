{
  "name": "@saas-accelerator/installer",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "aws-saas-parallel-deployments": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -rf yarn.lock && rm -rf dist && rm -rf cdk.out",
    "cleanup:tsc": "tsc --build ./ --clean",
    "lint": "eslint --fix  --max-warnings 0 -c ../../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "test": "jest --coverage --ci",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.312.0",
    "@aws-sdk/client-codepipeline": "3.312.0",
    "@aws-sdk/client-dynamodb": "3.312.0",
    "@aws-sdk/client-ec2": "3.312.0",
    "aws-cdk-lib": "2.72.0",
    "cdk-nag": "2.25.11",
    "constructs": "10.0.12",
    "source-map-support": "^0.5.16",
    "vm2": "^3.9.16"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@types/node": "18.11.19",
    "aws-cdk": "2.72.0",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "~5.0.4"
  }
}